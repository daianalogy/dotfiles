#  - ~/.config/alacritty/catppuccin/catppuccin-frappe.yml
#  - ~/.config/alacritty/catppuccin/catppuccin-latte.yml
colors:
  name: Epiphany
  author: ""
  primary:
    background: "#000000"
    foreground: "#ffffff"
  cursor:
    text: "#000000"
    cursor: "#ffffff"
  normal:
    black: "#565656"
    red: "#ac4f4f"
    green: "#88ab77"
    yellow: "#d0d9ad"
    blue: "#677088"
    magenta: "#cdcdb4"
    cyan: "#445362"
    white: "#ffffff"
  bright:
    black: "#656565"
    red: "#e85555"
    green: "#8ccd6c"
